<% if params[:query].nil?  %>
  <h1>Don't know where to start? Go with Jimi</h1>
<% end %>
<% @videos.each do |video| %>
  <img src="<%= video[:image_url_high] %>" alt="">
  <%= video[:title] %>
  <%= link_to "Go to Video", video_path(video[:video_id]) %>
  <%= video[:video_id] %><br><br>
<% end %>
